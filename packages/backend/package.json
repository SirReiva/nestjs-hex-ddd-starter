{
	"name": "project-backend",
	"version": "0.0.1",
	"description": "project",
	"author": "Agencia MPD",
	"private": true,
	"scripts": {
		"prebuild": "rimraf dist && npm run copy:utils",
		"build": "nest build",
		"copy:utils": "cpx \"../utils/**/*\" \"./src/utils\"",
		"doc": "npx @compodoc/compodoc -p tsconfig.json --theme material",
		"doc:dev": "npx @compodoc/compodoc -p tsconfig.json -s -o --theme material -w",
		"docker:dev:up": "docker-compose -p project-dev up -d",
		"docker:dev:down": "docker-compose -p project-dev down",
		"docker:dev:down:volumes": "docker-compose -p project-dev down -v",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"prestart:dev": "rimraf dist && npm run docker:dev:up && npm run copy:utils",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@golevelup/nestjs-rabbitmq": "^1.16.0",
		"@nestjs-modules/ioredis": "^1.0.0",
		"@nestjs/common": "^7.6.12",
		"@nestjs/config": "^0.6.3",
		"@nestjs/core": "^7.6.12",
		"@nestjs/cqrs": "^7.0.1",
		"@nestjs/graphql": "^7.9.9",
		"@nestjs/jwt": "^7.2.0",
		"@nestjs/mongoose": "^7.2.3",
		"@nestjs/platform-express": "^7.6.12",
		"@nestjs/serve-static": "^2.1.4",
		"apollo-server-express": "^2.21.0",
		"bcrypt": "^5.0.0",
		"figlet": "^1.5.0",
		"graphql-upload": "11.0.0",
		"ioredis": "^4.22.0",
		"mongoose": "^5.11.17",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^6.6.3",
		"uuid-random": "^1.3.2"
	},
	"devDependencies": {
		"@compodoc/compodoc": "^1.1.11",
		"@nestjs/cli": "^7.5.5",
		"@nestjs/schematics": "^7.2.7",
		"@nestjs/testing": "^7.6.12",
		"@types/amqp-connection-manager": "^2.0.10",
		"@types/bcrypt": "^3.0.0",
		"@types/express": "^4.17.11",
		"@types/figlet": "^1.2.1",
		"@types/ioredis": "^4.19.4",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.30",
		"@types/supertest": "^2.0.10",
		"cpx": "^1.5.0",
		"jest": "^26.6.3",
		"papaparse": "^5.3.0",
		"prettier": "^2.2.1",
		"supertest": "^6.1.3",
		"ts-jest": "^26.5.1",
		"ts-loader": "^8.0.17",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.1.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
